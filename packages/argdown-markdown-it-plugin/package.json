{
  "name": "@argdown/markdown-it-plugin",
  "version": "1.6.0",
  "description": "Turns Argdown code fences into svg argument maps",
  "main": "dist/argdown-markdown-it-plugin.js",
  "scripts": {
    "deploy": "npm run test && npm run build && npm version patch && npm run push",
    "sandbox": "parcel serve ./test/sandbox.html",
    "type-check": "tsc --noEmit",
    "build": "npm run clean && npm run build:src && npm run build:esm",
    "build:esm": "tsc -m es6 --target ES2018 --outDir dist-esm",
    "build:src": "tsc",
    "docs:build": "node ./node_modules/typedoc/bin/typedoc",
    "test": "mocha -r ts-node/register --full-trace --bail --require source-map-support/register ./test/**/*.spec.ts",
    "coverage": "nyc mocha -r ts-node/register --full-trace --bail --require source-map-support/register ./test/**/*.spec.ts",
    "report": "nyc report",
    "clean": "rimraf \"dist/*\" && rimraf \"dist-esm/*\"",
    "prepublishOnly": "npm run-script build",
    "push": "git push --tags origin HEAD:master"
  },
  "keywords": [
    "markdown-it-plugin",
    "markdown-it",
    "argdown",
    "argument maps",
    "argumentation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/christianvoigt/argdown/tree/master/packages/argdown-markdown-it-plugin"
  },
  "author": {
    "name": "Christian Voigt",
    "email": "1pxsolidblack@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@argdown/web-components": "^1.6.0",
    "@parcel/config-default": "^2.0.0-nightly.598",
    "@parcel/optimizer-cssnano": "2.0.0-nightly.598",
    "@parcel/optimizer-htmlnano": "2.0.0-nightly.598",
    "@parcel/packager-css": "2.0.0-nightly.598",
    "@parcel/packager-html": "2.0.0-nightly.598",
    "@parcel/transformer-css": "2.0.0-nightly.598",
    "@parcel/transformer-html": "2.0.0-nightly.598",
    "@parcel/transformer-postcss": "2.0.0-nightly.598",
    "@parcel/transformer-posthtml": "2.0.0-nightly.598",
    "@parcel/transformer-typescript-tsc": "^2.0.0-nightly.598",
    "@parcel/validator-typescript": "^2.0.0-nightly.598",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^8.0.0",
    "chai": "^4.2.0",
    "markdown-it": "^12.0.4",
    "mocha": "^8.0.1",
    "nyc": "^15.0.0",
    "parcel": "^2.0.0-nightly.596",
    "postcss": "^8.2.1",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.16",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@argdown/core": "^1.6.0",
    "@types/lodash.defaultsdeep": "^4.6.6",
    "@types/markdown-it": "^12.0.1",
    "lodash.defaultsdeep": "^4.6.1"
  },
  "browserslist": "> 0.2%"
}
