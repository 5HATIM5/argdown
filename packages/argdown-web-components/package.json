{
  "name": "@argdown/web-components",
  "version": "1.4.1",
  "description": "Components that help to embed Argdown in the browser",
  "module": "dist-esm/argdown-map.js",
  "browser": "dist/argdown-map.js",
  "source": "src/argdown-map.ts",
  "targets": {
    "browser": {
      "context": "browser",
      "includeNodeModules": true,
      "isLibrary": true,
      "outputFormat": "esmodule",
      "engines": {
        "browsers": [
          "last 1 Chrome version"
        ]
      }
    },
    "module": {
      "context": "browser",
      "includeNodeModules": false,
      "isLibrary": true,
      "engines": {
        "browsers": [
          "> 0.25%"
        ]
      }
    }
  },
  "scripts": {
    "start": "parcel src/index.html --open",
    "build": "yarn run clean && parcel build src/argdown-map.ts",
    "clean": "rimraf \"dist/*\" && rimraf \"dist-esm/*\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/christianvoigt/argdown/tree/master/packages/argdown-web-components"
  },
  "author": {
    "name": "Christian Voigt",
    "email": "1pxsolidblack@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "d3-selection": "^1.4.1",
    "d3-zoom": "^1.8.3",
    "haunted": "^4.7.0",
    "lit-html": "^1.2.1"
  },
  "devDependencies": {
    "@webcomponents/webcomponentsjs": "^2.4.3",
    "@types/d3": "^5.7.2",
    "parcel": "^2.0.0-alpha.3.2",
    "parcel-plugin-static-files-copy": "^2.3.1",
    "typescript": "^3.8.3",
    "rimraf": "^3.0.2"
  },
  "browserslist": [
    "Chrome > 50"
  ]
}
