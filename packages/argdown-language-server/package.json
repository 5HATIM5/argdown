{
  "name": "@argdown/language-server",
  "description": "Language Server for the Argdown argumentation syntax.",
  "version": "1.4.5",
  "author": "Christian Voigt",
  "license": "MIT",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "files": [
    "dist/"
  ],
  "engines": {
    "node": ">= 8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/christianvoigt/argdown/tree/master/packages/argdown-language-server"
  },
  "scripts": {
    "build": "npm run build-webpack",
    "build-ts": "tsc -p .",
    "build-webpack": "npm run clean && webpack --config webpack.config.js --mode production",
    "clean": "rimraf \"dist/*\"",
    "watch": "tsc -w -p .",
    "prepublishOnly": "npm run build",
    "test": "mocha -r ts-node/register --full-trace --require source-map-support/register ./test/**/*.spec.ts"
  },
  "devDependencies": {
    "@argdown/core": "^1.4.5",
    "@argdown/node": "^1.4.5",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/webpack": "^4.41.12",
    "chai": "^4.2.0",
    "mocha": "^7.1.1",
    "raw-loader": "^4.0.1",
    "rimraf": "^3.0.2",
    "string-replace-webpack-plugin": "^0.1.3",
    "transform-loader": "^0.2.4",
    "ts-loader": "^7.0.2",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3",
    "vscode-languageserver": "^6.1.1",
    "vscode-languageserver-textdocument": "^1.0.1",
    "vscode-uri": "^2.1.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
