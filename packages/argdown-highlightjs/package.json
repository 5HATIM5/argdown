{
  "name": "@argdown/highlightjs",
  "version": "1.7.0",
  "description": "Argdown language definition for highlightjs",
  "main": "dist/index.js",
  "module": "dist-esm/index.js",
  "files": [
    "dist/theme.css"
  ],
  "scripts": {
    "sandbox": "parcel ./test/sandbox.html",
    "build": "npm run clean && node copy-css.js && npm run build:src && npm run build:esm",
    "build:esm": "tsc -m es6 --target ES2018 --outDir dist-esm",
    "build:src": "tsc",
    "clean": "rimraf \"dist/*\" && rimraf \"dist-esm/*\"",
    "copy-css": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/christianvoigt/argdown/tree/master/packages/argdown-highlightjs"
  },
  "keywords": [
    "Argdown",
    "highlightjs"
  ],
  "author": "Christian Voigt",
  "license": "MIT",
  "devDependencies": {
    "@parcel/config-default": "^2.0.0-nightly.612",
    "@parcel/optimizer-cssnano": "^2.0.0-nightly.612",
    "@parcel/optimizer-htmlnano": "^2.0.0-nightly.612",
    "@parcel/packager-css": "^2.0.0-nightly.612",
    "@parcel/packager-html": "^2.0.0-nightly.612",
    "@parcel/transformer-css": "^2.0.0-nightly.612",
    "@parcel/transformer-html": "^2.0.0-nightly.612",
    "@parcel/transformer-postcss": "^2.0.0-nightly.612",
    "@parcel/transformer-posthtml": "^2.0.0-nightly.612",
    "@parcel/transformer-typescript-tsc": "^2.0.0-nightly.612",
    "@parcel/validator-typescript": "^2.0.0-nightly.612",
    "highlight.js": "^10.0.1",
    "parcel": "^2.0.0-nightly.610",
    "postcss": "^8.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  }
}
